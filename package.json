  "scripts": {
    "start": "vite --host --open",
    "build": "yarn clean && tsc --noEmit && vite build",
    "build:types": "dts-bundle-generator --config ./dts-bundle-generator.config.ts --no-banner",
    "clean": "rimraf dist storybook-static .stylelintcache node_modules/.cache node_modules/.vite",
    "reset": "rimraf node_modules yarn.lock && yarn install",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint:scripts": "eslint ./src --ext .ts,.tsx",
    "lint:scripts:fix": "eslint ./src --ext .ts,.tsx --fix",
    "lint:styles": "stylelint ./src/*.{css,scss}",
    "format:scripts": "prettier ./src --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
